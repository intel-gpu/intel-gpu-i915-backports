#!/bin/sh
#
#postinst script for intel-i915

set -e

#update initramfs
case "$1" in
    configure)
	depmod -a
	if [ -x /usr/sbin/update-initramfs ]; then
	    /usr/sbin/update-initramfs -u
	fi
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
	echo "postinst called with unknown argument \`$1'" >&2
	exit 1
    ;;
esac

exit 0
